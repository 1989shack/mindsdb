{
    "debug": true,
    "use_gpu": false,
    "config_version": 1,
    "python_interpreter": "/usr/bin/python3",
    "api": {
        "mysql": {
            "port": 47335,
            "host": "127.0.0.1",
            "user": "mindsdb",
            "password": "mindsdb",
            "certificate_path": "cert.pem",
            "log": {
                "format": "%(asctime)s - %(levelname)s - %(message)s",
                "folder": "logs/",
                "file": "mysql.log",
                "file_level": "INFO",
                "console_level": "INFO"
            },
            "datasources": []
        },
        "http": {
          "port": 47334
          ,"host": "0.0.0.0"
        }
    }
    ,"integrations": {
      "clickhouse": {
          "enabled": true
          ,"host": "localhost"
          ,"port": 8123
          ,"user": "default"
          ,"password": ""
      }
    }
    ,"interface":{
      "mindsdb_native": {
          "enabled": true
          ,"storage_dir": "/var/lib/mindsdb/predictors/"
      }
      ,"lightwood": {
           "enabled": true
      }
      ,"datastore": {
           "enabled": true
           ,"storage_dir": "/var/lib/mindsdb/datasources/"
      }
      ,"dataskillet": {
           "enabled": false
      }
    }
}
